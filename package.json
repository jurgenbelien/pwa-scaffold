{
  "name": "pwa-scaffold",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel watch serve",
    "preserve": "npm run build",
    "serve": "sirv ./dist",
    "prebuild": "rimraf dist/*",
    "build": "npm-run-all build:*",
    "build:clear": "rimraf dist/*",
    "build:images": "cp -R assets dist/",
    "build:manifest": "cp manifest.json dist/",
    "build:main": "rollup --config",
    "build:sw": "rollup --config=rollup.config.sw.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:main": "rollup --config --watch",
    "watch:sw": "rollup --config=rollup.config.sw.js --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jurgen BeliÃ«n",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-html": "^0.2.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "autoprefixer": "^10.2.5",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.15",
    "postcss-custom-properties": "^11.0.0",
    "postcss-import": "^14.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0",
    "rollup-plugin-output-manifest": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "sirv-cli": "^1.0.11",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}
